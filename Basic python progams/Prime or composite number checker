number=int(input("Enter the number u want to input  "))
if(number==0):
           print("The number is neither composite nor prime")
           exit
elif(number>0):
	for i in range(2,number):
		if (number% i) == 0:
			print(number, "is not a prime number")
			break
	else:
		print(number, "is a prime number")
else:
	print(number, "is not a prime number")
